{
  "mcpServers": {
    "filesystem": {
      "command": "node",
      "args": [
        "dist/index.js",
        "npx",
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/hisuzuya/Documents/app/rayven/tumiki-proxy"
      ],
      "env": {
        "TUMIKI_LOG_FILE": "/tmp/mcp-filesystem.log"
      }
    },
    "context7": {
      "command": "node",
      "args": [
